<?xml version="1.0" encoding="UTF8"?>
<manifest name="accords_provisioning" xmlns="http://www.compatibleone.fr/schemes/cords.xsd">
<description>This manifest describes an operation provisioning system buiolt on the basic accords platform.</description>
<node name="accords" access="public" scope="common" type="accords"/>
<node name="osprocci" access="public" scope="common"/>
<node name="onprocci" access="public" scope="common"/>
<configuration name="accords_provisioning">
<action name="osprocci" type="cordscript" 
expression="osprocci.configure(accords.publisher.hostname);osprocci.configure(accords.coss.hostname);osprocci.configure(accords.comons.hostname);"/>
<action name="onprocci" type="cordscript" 
 expression="onprocci.configure(accords.publisher.hostname);onprocci.configure(accords.coss.hostname);onprocci.configure(accords.comons.hostname);"/>
</configuration>
<security name="public" level="public"/>
<account name="public"/>
</manifest>
